#!/usr/bin/env bash

set -e

if ! which -s jsx ; then
    echo "You must install jsx, like:"
    echo "  npm install -g react-tools"
    exit 2
fi

bin="$(dirname $BASH_SOURCE)"
bin="$(cd $bin ; pwd)"
base="$(dirname $bin)"
echo jsx --watch data/sidebar/jsx data/sidebar/js
jsx --watch data/sidebar/jsx data/sidebar/js
